<IfModule mod_rewrite.c>
  RewriteEngine On
  
  # SETTING UNTUK ROOT DOMAIN / SUBDOMAIN
  # Jika diakses di arsip.uptsdnremen2.sch.id langsung
  RewriteBase /

  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . index.html [L]
</IfModule>
